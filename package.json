{
  "name": "ts-json-decoder",
  "version": "1.0.0",
  "description": "Typescript JSON Decoder",
  "main": "lib/decoder.js",
  "types": "lib/decoder.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SyloRei/ts-json-decoder.git"
  },
  "keywords": [
    "Typescript",
    "JSON",
    "Decoder"
  ],
  "author": "SyloRei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SyloRei/ts-json-decoder/issues"
  },
  "homepage": "https://github.com/SyloRei/ts-json-decoder#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  }
}
